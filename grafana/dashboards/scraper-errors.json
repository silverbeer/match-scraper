{
  "dashboard": {
    "title": "MLS Match Scraper - Errors & Debugging",
    "description": "Error tracking and debugging dashboard for MLS Match Scraper",
    "tags": ["mls-scraper", "errors", "debugging"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Errors (Last Hour)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(increase(scraping_errors_total{service=\"mls-match-scraper\"}[1h]))",
            "legendFormat": "Errors"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Failed API Calls (Last Hour)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(increase(api_calls_total{service=\"mls-match-scraper\",status_class=~\"4xx|5xx\"}[1h]))",
            "legendFormat": "Failed Calls"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "gauge",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "(sum(rate(scraping_errors_total{service=\"mls-match-scraper\"}[5m])) / sum(rate(browser_operations_total{service=\"mls-match-scraper\"}[5m]))) * 100",
            "legendFormat": "Error %"
          }
        ],
        "options": {
          "orientation": "auto",
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 15, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Time Since Last Successful Run",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "time() - max(application_execution_duration_seconds)",
            "legendFormat": "Seconds"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 86400, "color": "yellow"},
                {"value": 172800, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Errors Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 4
        },
        "targets": [
          {
            "expr": "sum by (error_type) (rate(scraping_errors_total{service=\"mls-match-scraper\"}[5m]))",
            "legendFormat": "{{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "auto",
              "stacking": {
                "mode": "normal"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "table",
            "placement": "right",
            "calcs": ["sum", "lastNotNull"]
          }
        }
      },
      {
        "id": 6,
        "title": "Failed API Calls by Endpoint",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "sum by (endpoint, method, status_code) (increase(api_calls_total{service=\"mls-match-scraper\",status_class=~\"4xx|5xx\"}[6h]))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "endpoint": 0,
                "method": 1,
                "status_code": 2,
                "Value": 3
              },
              "renameByName": {
                "endpoint": "Endpoint",
                "method": "Method",
                "status_code": "Status",
                "Value": "Count"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Count",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "Browser Operation Failures",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "sum by (operation) (increase(browser_operations_total{service=\"mls-match-scraper\",success=\"false\"}[6h]))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "renameByName": {
                "operation": "Operation",
                "Value": "Failures"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Failures",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Recent Error Logs",
        "type": "logs",
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "{job=\"mls-match-scraper\"} |= \"ERROR\"",
            "refId": "A"
          }
        ],
        "options": {
          "showLabels": false,
          "showCommonLabels": false,
          "showTime": true,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      }
    ]
  }
}
